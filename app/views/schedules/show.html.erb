
<h2><%= t('.schedule_details') %></h2><br>

<% unless notice.nil? %>
  <div class="container center alert alert-success"  style="width:500px">
    <p id="notice"><%= notice %></p>
  </div>
<% end %>

<% if flash[:alert] %>
  <div class="container center alert alert-error"  style="width:500px">
    <p id="alert"><%= flash[:alert] %></p>
  </div>
<% end %>

<table class="table table-striped table-bordered table-condensed">
<tr>
  <td align="right" style="width:200px"><b><%= t('.schedule_name') %></b></td>
  <td><%= @schedule.sch_name %></td>
</tr>
<tr>
  <td align="right" style="width:200px"><b><%= t('.schedule_cronspec') %></b></td>
  <td><%= @schedule.sch_cronspec %></td>
</tr>
<tr>
  <td align="right" style="width:200px"><b><%= t('.schedule_status') %></b></td>
  <td><%= raw get_ScheduleStatusText(@schedule.sch_status) %></td>
</tr>
<tr>
  <td align="right" style="width:200px"><b><%= t('.schedule_user') %></b></td>
  <td><%= get_UserName(@schedule.sch_user) %></td>
</tr>
<tr>
  <td align="right" style="width:200px"><b><%= t('.schedule_action') %></b></td>
  <td><%= @schedule.sch_action %></td>
</tr>
</table>

<%= link_to t('.edit_schedule'), edit_schedule_path(@schedule), :class=>'btn btn-primary btn-mini' %>

<br>
<%= link_to t('.back'), schedules_path %>




